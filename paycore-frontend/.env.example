# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Application Configuration
VITE_APP_NAME=PayCore
VITE_APP_VERSION=1.0.0

# Firebase (for push notifications - optional)
# Get these from: https://console.firebase.google.com/ > Project Settings > General
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
# Get VAPID key from: https://console.firebase.google.com/ > Project Settings > Cloud Messaging > Web Push certificates
VITE_FIREBASE_VAPID_KEY=

# Google OAuth (optional)
VITE_GOOGLE_CLIENT_ID=

# Feature Flags
VITE_ENABLE_BIOMETRIC_AUTH=true
VITE_ENABLE_OAUTH=true
VITE_ENABLE_DARK_MODE=true

# Analytics (optional)
VITE_GA_TRACKING_ID=
